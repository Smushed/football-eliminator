{
    "name": "footballeliminator",
    "version": "0.2.0",
    "description": "Fantasy Football Eliminator",
    "main": "server.js",
    "engines": {
        "node": "10.X.X"
    },
    "scripts": {
        "start:testings": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
        "start:prod": "node server.js",
        "start": "concurrently \"nodemon --ignore client/\" \"npm run client\"",
        "client": "cd ./client && npm run start",
        "install": "cd ./client && npm install",
        "build": "cd ./client && npm run build",
        "heroku-postbuild": "npm run build"
    },
    "author": "Smushed",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "^4.1.2",
        "nodemon": "^1.19.4"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "body-parser": "^1.19.0",
        "bootstrap": "^4.3.1",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "dotenv": "^6.2.0",
        "if-env": "^1.0.4",
        "express": "^4.17.1",
        "mongoose": "^5.7.11",
        "uuid": "^3.3.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/football-eliminator/football-eliminator.git"
    },
    "keywords": [],
    "bugs": {
        "url": "https://github.com/football-eliminator/football-eliminator/issues"
    },
    "homepage": "https://github.com/football-eliminator/football-eliminator#readme"
}